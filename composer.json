{
    "name": "shima-freebz/BarCodeQR",
    "description": "BarCodeQR plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.5.9",
        "cakephp/cakephp": ">=3.3.2 <4.0.0",
        "picqer/php-barcode-generator": "^0.2.1",
        "endroid/qrcode": "^1.9"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "BarCodeQR\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BarCodeQR\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
